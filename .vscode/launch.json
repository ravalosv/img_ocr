{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "easyocr",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/img_easy_ocr.py",
      "console": "integratedTerminal",
      "justMyCode": true,
      "cwd": "${workspaceFolder}",
      "env": {
        "PYTHONPATH": "${workspaceFolder}"
      },
      "stopOnEntry": false,
      "python": "${command:python.interpreterPath}"
    }
  ]
}
